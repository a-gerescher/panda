{
  "name": "panda",
  "version": "0.0.1",
  "description": "A simple opinionated notetaking app",
  "main": "index.js",
  "scripts": {
    "build-old": "rollup -c --environment NODE_ENV:production --environment mod:iife && rollup -c --environment NODE_ENV:production --environment mod:esm",
    "build": "rollup -c --environment NODE_ENV:production",
    "watch": "rollup -c --watch",
    "dev": "run-p -rlsn --npm-path yarn watch serve",
    "serve": "servor build index.html 8084 --reload"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "cp-file": "^9.0.0",
    "esbuild": "^0.6.32",
    "fdir": "^4.1.0",
    "pify": "^5.0.0",
    "preact": "^10.4.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.11",
    "rollup-plugin-esbuild": "^2.5.0",
    "sass": "^1.26.10",
    "terser": "^5.3.0"
  },
  "eslintIgnore": [
    "package.json",
    "rollup.*.js",
    "build/**/*.js",
    "src/preact.min.js"
  ],
  "snowpack": {
    "webDependencies": [
      "preact",
      "preact/hooks"
    ]
  },
  "dependencies": {
    "marked": "^2.0.0",
    "prop-types": "^15.7.2",
    "state-local": "^1.0.1"
  }
}
